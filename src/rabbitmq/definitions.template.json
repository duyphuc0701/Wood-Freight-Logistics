{
  "vhosts": [
    {
      "name": "/"
    }
  ],
  "queues": [
    {
      "name": "gps_queue",
      "vhost": "/",
      "durable": true
    },
    {
      "name": "fault_queue",
      "vhost": "/",
      "durable": true
    }
  ],
  "users": [
    {
      "name": "${RABBITMQ_USER}",
      "password": "${RABBITMQ_PASSWORD}",
      "tags": "administrator"
    }
  ],
  "permissions": [
    {
      "user": "${RABBITMQ_USER}",
      "vhost": "/",
      "configure": ".*",
      "write": ".*",
      "read": ".*"
    }
  ]
}
